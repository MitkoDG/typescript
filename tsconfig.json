{
  "compilerOptions": {
    "target": "ESNext",
    "module": "CommonJS",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "allowJs": true,
    "checkJs": true,
    "sourceMap": false,
    "removeComments": true,
    "outDir": "dist",
    // "noLib": 
    // "lib": [
    //   "ES5",
    //   "DOM"
    // ]
    "baseUrl": "",
    "strictPropertyInitialization": false,
  },
  // "files": [
  //   "demo.ts"
  // ],
  "include": ["*.ts"],
  "exclude": ["node_modules"]
}
